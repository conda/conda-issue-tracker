{
  "name": "project-has-issue",
  "version": "0.1.0",
  "description": "Detect whether an issue exists in the specified project.",
  "main": "lib/main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "node -p \"'export const LIB_VERSION: string = ' + JSON.stringify(require('./package.json').version)\" > src/version.ts",
    "build": "tsc && ncc build",
    "rebuild": "npm run clean && npm run build",
    "dist": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "dist:re": "npm run clean && npm run dist",
    "dev": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Anaconda Inc.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "commander": "^8.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@vercel/ncc": "^0.33.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  }
}
